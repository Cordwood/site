---
import "unocss-hmr-fix";

export interface Props {
    width?: string;
    height?: string;
    variant: "black" | "white";
}

const { width, height, variant } = Astro.props;
---

<img class="self-center" width={width} height={height} src={`//raw.githubusercontent.com/Cordwood/Cordwood/master/.assets/banner/cordwood-transparent${variant === "black" ? "-black" : ""}.png`} />